<?xml version="1.0" ?>
<robot name="orne_box" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find icart_mini_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find orne_box_description)/urdf/orne_box.gazebo.xacro"/>

  <link name="base_link">
    <visual>
<!--
      <origin xyz="0 0 -0.154" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://orne_box_description/meshes/bases/orne_box_base.stl" scale="1 1 1"/>
        <box size="0.540 0.404 0.202"/>
      </geometry>
-->
      <origin xyz="0.090 0 0.0195" rpy="0 0 0"/>
      <geometry>
        <box size="0.400 0.404 0.202"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <visual>
      <origin xyz="0.36125 0 0.1105" rpy="0 0 0"/>
      <geometry>
        <box size="0.1425 0.404 0.020"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <visual>
      <origin xyz="0.35625 0 0.004" rpy="0 0 0"/>
      <geometry>
        <box size="0.1325 0.404 0.020"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <visual>
      <origin xyz="-0.120 0 0.3295" rpy="0 0 0"/>
      <geometry>
        <box size="0.020 0.404 0.822"/>
      </geometry>
      <material name="orange"/>
    </visual>
    
    <collision>
      <origin xyz="0.090 0 0.0195" rpy="0 0 0"/>
      <geometry>
        <box size="0.400 0.404 0.202"/>
      </geometry>
    </collision>
    <collision>
      <origin xyz="0.36125 0 0.1105" rpy="0 0 0"/>
      <geometry>
        <box size="0.1425 0.404 0.020"/>
      </geometry>
    </collision>
    <collision>
      <origin xyz="0.35625 0 0.004" rpy="0 0 0"/>
      <geometry>
        <box size="0.1325 0.404 0.020"/>
      </geometry>
    </collision>
    <collision>
      <origin xyz="-0.120 0 0.3295" rpy="0 0 0"/>
      <geometry>
        <box size="0.020 0.404 0.822"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0.07 -0.05" rpy="0 0 0"/>
      <mass value="25.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <joint name="left_wheel_hinge" type="revolute">
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <origin xyz="0.0 0.23075 0.0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" velocity="100.0"  lower="-5000" upper="5000" />
  </joint>

  <link name="wheel_left_link">
    <visual>
<!--
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://orne_box_description/meshes/wheels/tire.stl" scale="1 1 1"/>
      </geometry>
-->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.045" radius="0.152"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.045" radius="0.152"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
<!--
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
-->
    </inertial>

  </link>

  <joint name="right_wheel_hinge" type="revolute">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0.0 -0.23075 0.0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" velocity="100.0"  lower="-5000" upper="5000" />
  </joint>

  <link name="wheel_right_link">
    <visual>
<!--
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://orne_box_description/meshes/wheels/tire.stl" scale="1 1 1"/>
      </geometry>
-->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.045" radius="0.152"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.045" radius="0.152"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
<!--
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
             iyy="1.1192413e-05" iyz="-1.4400107e-11"
             izz="2.0712558e-05" />
-->
    </inertial>

  </link>

  <joint name="caster_front_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_front_link"/>
    <origin xyz="0.362 0 -0.102" rpy="0 0 0"/>
  </joint>

  <link name="caster_front_link">
    <visual>
      <origin xyz="0 0.150 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.025" radius="0.050"/>
      </geometry>
      <material name="red"/>
    </visual>
    <visual>
      <origin xyz="0 -0.150 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.025" radius="0.050"/>
      </geometry>
      <material name="red"/>
    </visual>

    <collision>
      <origin xyz="0 0.150 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.025" radius="0.050"/>
      </geometry>
    </collision>
    <collision>
      <origin xyz="0 -0.150 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.025" radius="0.050"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.2" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
    </inertial>
  </link>

  <joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0.3 0 0.1225" rpy="0 0 -1.57"/>
  </joint>

  <link name="imu_link"/>

  <joint name="scan_joint" type="fixed">
    <parent link="base_link"/>
    <child link="hokuyo_link"/>
    <origin xyz="0.374295 0 0.1225" rpy="-3.14 0 0"/>
  </joint>

  <link name="hokuyo_link">

    <inertial>
      <mass value="0.08" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1" />
    </inertial>
  </link>

  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_wheel_hinge">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
    </joint>   
  <actuator name="left_motor">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="right_wheel_hinge">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
    </joint>  
  <actuator name="right_motor">
      <hardwareInterface>VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

</robot>
