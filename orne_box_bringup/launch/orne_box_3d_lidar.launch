<?xml version="1.0"?>
<launch>
  <arg name="enable_zed" default="true"/>
  <include file="$(find icart_mini_driver)/launch/icart_mini_drive.launch">
    <arg name="model" value="$(find xacro)/xacro.py '$(find orne_box_description)/urdf/orne_box_3d_lidar.urdf.xacro'"/>
    <arg name="ypspur_params" value="$(find orne_box_setup)/config/orne_box.param"/>
    <arg name="use_eth_urg" value="true"/>
    <arg name="urg_ip" value="192.168.0.10"/>
  </include>
  <include file="$(find orne_box_bringup)/launch/includes/3d_base.launch.xml"/>
  <include file="$(find orne_box_bringup)/launch/includes/rfans16.launch"/>
  <include file="$(find zed_wrapper)/launch/zed_no_tf.launch" if="$(arg enable_zed)">
    <arg name="camera_name" value="zed"/>
    <arg name="camera_model" value="zed"/>
  </include>
</launch>
